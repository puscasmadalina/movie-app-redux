data = action.payload;\n    },\n\n    loadingMore: (state) => {\n      state.loadMore.page += 1;\n    },\n  },\n});\n\nexport const {\n  openModal,\n  closeModal,\n  trendingMovies,\n  trendingTvShows,\n  loadingMore,\n} = AppSlice.actions;\n\nexport default AppSlice.reducer;\n"],"mappings":"AAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,YAAY,CAAG,CACnBC,KAAK,CAAE,CACLC,MAAM,CAAE,KAAK,CACbC,SAAS,CAAE,EACb,CAAC,CAEDC,aAAa,CAAE,CACbC,IAAI,CAAE,IACR,CAAC,CACDC,UAAU,CAAE,CACVD,IAAI,CAAE,IACR,CAAC,CAEDE,QAAQ,CAAE,CACRC,IAAI,CAAE,CACR,CACF,CAAC,CAED,MAAO,MAAM,CAAAC,QAAQ,CAAGV,WAAW,CAAC,CAClCW,IAAI,CAAE,KAAK,CACXV,YAAY,CACZW,QAAQ,CAAE,CACRC,SAAS,CAAEA,CAACC,KAAK,CAAEC,MAAM,GAAK,CAC5BC,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC,CAC7BH,KAAK,CAACZ,KAAK,CAACC,MAAM,CAAG,IAAI,CACzBW,KAAK,CAACZ,KAAK,CAACE,SAAS,CAAGW,MAAM,CAACG,OAAO,CACxC,CAAC,CACDC,UAAU,CAAGL,KAAK,EAAK,CACrBE,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC,CAC5BH,KAAK,CAACZ,KAAK,CAACC,MAAM,CAAG,KAAK,CAC5B,CAAC,CACDiB,cAAc,CAAEA,CAACN,KAAK,CAAEC,MAAM,GAAK,CACjCD,KAAK,CAACT,aAAa,CAACC,IAAI,CAAGS,MAAM,CAACG,OAAO,CAC3C,CAAC,CACDG,eAAe,CAAEA,CAACP,KAAK,CAAEC,MAAM,GAAK,CAClCD,KAAK,CAACP,UAAU,CAACD,IAAI,CAAGS,MAAM,CAACG,OAAO,CACxC,CAAC,CAEDI,WAAW,CAAGR,KAAK,EAAK,CACtBA,KAAK,CAACN,QAAQ,CAACC,IAAI,EAAI,CAAC,CAC1B,CACF,CACF,CAAC,CAAC,CAEF,MAAO,MAAM,CACXI,SAAS,CACTM,UAAU,CACVC,cAAc,CACdC,eAAe,CACfC,WACF,CAAC,CAAGZ,QAAQ,CAACa,OAAO,CAEpB,cAAe,CAAAb,QAAQ,CAACc,OAAO"},"metadata":{},"sourceType":"module","externalDependencies":[]}